  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "cssnano": "^3.10.0",
    "d3": "^4.7.4",
    "echarts": "^3.5.1",
    "highcharts": "^5.0.10",
    "http-server": "^0.9.0",
    "js-beautify": "^1.6.11",
    "jshint": "^2.9.4",
    "npm-run-all": "^4.0.1",
    "postcss-cssnext": "^2.9.0",
