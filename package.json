{
  "name": "gerui",
  "version": "1.0.0",
  "description": "GER-UI",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "watch": "./node_modules/.bin/rollup -c --watch",
    "server": "http-server -a 0.0.0.0 -p 8889 -s",
    "devwatch": "rollup -c  rollup.config.js -w",
